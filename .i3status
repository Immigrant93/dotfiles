general {
    colors = true
    color_good = "#CCCCCC"
    color_bad = "#cc6666"
    color_degraded = "#55858E"
    interval = 2
}

order += "battery 0"
order += "tztime local"

battery 0 {
        format = " %percentage %remaining"
        last_full_capacity = true
}

tztime local {
        format = " %d/%m/%Y %H:%M"
}
